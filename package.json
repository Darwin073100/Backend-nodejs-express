{
  "name": "backend-nodejs-express",
  "version": "1.0.0",
  "description": "Backend con node.js, express y postgresql",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "lint": "eslint",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli bd:migrate:undo"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "expressjs",
    "postgresql"
  ],
